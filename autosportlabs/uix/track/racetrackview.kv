#:kivy 1.9.0
#:import ColorScheme autosportlabs.racecapture.theme.color.ColorScheme

<RaceTrackView>:
	orientation: 'vertical'
	Scatter:
        id: scatter
        TrackMap:
            trackColor: ColorScheme.get_primary()
            height: root.height
            width: root.width
            id: trackmap
